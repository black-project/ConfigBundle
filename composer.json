{
    "name": "black/config-bundle",
    "type": "symfony-bundle",
    "description": "Symfony BlackConfigBundle",
    "keywords": ["Bundle", "Symfony", "Black"],
    "homepage": "http://www.lablackroom.com",
    "license": "MIT",
    "authors": [
        {
            "name": "Alexandre Balmes",
            "email": "abalmes@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.4.0",
        "black/common-bundle": "~0.3",
        "black/assets-bundle": "~0.2"
    },
    "suggest": {
        "doctrine/mongodb-odm": "Needed when using mongo implementation",
        "doctrine/mongodb-odm-bundle": "Needed when using mongo implementation"
    },
    "autoload": {
        "psr-0": { "Black\\Bundle\\ConfigBundle": "" }
    },
    "target-dir": "Black/Bundle/ConfigBundle"
}
